  <body class='theme-color-purple theme-pattern-purple-marble'>
    <!-- Page Header -->
  <header id='masthead'>
      <nav class='navbar navbar-fixed-top'>
        <div class='navbar-inner'>
          <div class='container'>
            <a class='btn btn-navbar' data-target='.nav-collapse' data-toggle='collapse'>
